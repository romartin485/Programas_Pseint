Algoritmo Verificar_sexo
	
	Escribir "Digite su Nombre";
	Leer nombre;
	
	Escribir "Digite su Sexo";
	Leer sexo;
	
	Si sexo="f" o sexo="F" Entonces
		Escribir "Su Nombre es:Sra.", nombre;
	FinSi
	
	Si sexo="m" o sexo="M" Entonces
		Escribir "Su Nombre es:Sr.", nombre;
	FinSi
	
FinAlgoritmo
